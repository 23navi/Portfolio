    <title>Navi's Projects</title>
   

    <script>

        /* Storing user's device details in a variable*/
        let details = navigator.userAgent;
  
        /* Creating a regular expression 
        containing some mobile devices keywords 
        to search it in details string*/
        let regexp = /android|iphone|kindle|ipad/i;
  
        /* Using test() method to search regexp in details
        it returns boolean value*/
        let isMobileDevice = regexp.test(details);
  
        if (isMobileDevice) {
            setTimeout(()=>{window.location.href = "/allProjects";},5000)
        } else {
            console.log("You are using Desktop");
        }

   
        function setupElements() {
            let hamburgerClicked = false;
            var element = select('button');
            element.touchStarted(clickedHamburger);
            element = select("body");
            element.touchStarted(clickedBody);
        }


        function myFunction() {
            document.getElementById("myDropdown").classList.toggle("show");
        }

        clickedBody = function (e) {
            if (!hamburgerClicked && windowWidth < 700) {
                var menuItems = document.getElementsByClassName("menuItems");
                menuItems[0].style.display = "none";
            }
            hamburgerClicked = true;
        };

        function clickedHamburger() {
            var menuItems = document.getElementsByClassName("menuItems");
            if (menuItems[0].style.display === "grid")
                return;
            menuItems[0].style.display = "grid";
            hamburgerClicked = true;
        }

        function onResize() {
            var menuItems = document.getElementsByClassName("menuItems");
            if (windowWidth > 700 && menuItems[0].style.display === "none") {

                menuItems[0].style.display = "grid";
            }
        }


    </script>


        



</head>
